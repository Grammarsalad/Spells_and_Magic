//Current hit points
APPEND ~splprot.2da~ ~B_CHP_GR%TAB%0%TAB%-1%TAB%3~  //current hp Greater than
APPEND ~splprot.2da~ ~B_CHP_EQ%TAB%0%TAB%-1%TAB%1~  //current hp equal to
APPEND ~splprot.2da~ ~B_CHP_LS%TAB%0%TAB%-1%TAB%2~  //current hp Less than

COPY_EXISTING ~splprot.2da~ ~override~
   COUNT_2DA_COLS cols // amount of columns
   READ_2DA_ENTRIES_NOW rows cols // read all file into memory  
   FOR (row = 1; row < rows; ++row) BEGIN // iterate over rows
      READ_2DA_ENTRY_FORMER rows row 0 ~stat~ // read column value
      PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~B_CHP_GR~ BEGIN  //hp>
         SET chp_gr = %row%
      END
      PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~B_CHP_EQ~ BEGIN  //str =
         SET chp_eq = %row%
      END
      PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~B_CHP_LS~ BEGIN  //str<
         SET chp_ls = %row%
      END
   END
BUT_ONLY